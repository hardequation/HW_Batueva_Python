    "def fib(n):\n",
    "    if n == 0:\n",
    "        return 0\n",
    "    elif n == 1:\n",
    "        return 1\n",
    "    else:\n",
    "        an1 = 0\n",
    "        an2 = 1\n",
    "        for i in range(n - 1):\n",
    "            an = an1 + an2\n",
    "            an1 = an2\n",
    "            an2 = an\n",
    "        return an2\n",
    "            \n",
    "n = int(input())\n",
    "print(fib(n))"
   ]
  }
